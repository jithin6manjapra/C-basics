#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num1,num2;
	int sum=0;
	printf("Enter first number");
	scanf("%d",&num1);
	printf("Enter Second number");
	scanf("%d",&num2);
	printf("sum=%d",sum);
	return EXIT_SUCCESS;
}
